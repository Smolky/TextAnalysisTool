								</dimension>						
							</dimensions>
						</dimension>
					</dimensions>
				</dimension>
				<dimension>
					<key>impersonal</key>
					<class>MatchingWordsFromDictionary</class>
					<dictionary>impersonal-pronouns</dictionary>
				</dimension>
			</dimensions>
		</dimension>
		<dimension>
			<key>articles</key>
			<dimensions>
