		<dimension>
			<key>lng-function</key>
			<dimensions>
				<dimension>
					<key>personal</key>
					<dimensions>
						<dimension>
							<key>FirstPerson</key>
							<dimensions>
								<dimension>
									<key>I</key>
									<class>MatchingWordsFromDictionary</class>
									<dictionary>personal-pronouns-first-person-singular</dictionary>
								</dimension>
								<dimension>
									<key>We</key>
									<class>MatchingWordsFromDictionary</class>
									<dictionary>personal-pronouns-first-person-plural</dictionary>
								</dimension>
							</dimensions>
						</dimension>
						<dimension>
							<key>You</key>
							<class>MatchingWordsFromDictionary</class>
							<dictionary>personal-pronouns-second-person</dictionary>
						</dimension>
						<dimension>
							<key>ThirdPerson</key>
							<dimensions>						
								<dimension>
									<key>ThirdSingular</key>
									<class>MatchingWordsFromDictionary</class>
									<dictionary>personal-pronouns-third-person-singular</dictionary>
