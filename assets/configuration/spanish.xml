<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	
	<directory_folder>assets/dictionaries/es/</directory_folder>
	
	
	<!-- Configuration -->
	<dimensions>
		<!-- Simple, specify the key -->
		<dimension>
			<key>wc</key>
		</dimension>
		<!-- Summary variable dimensions -->
		<dimension>
			<key>summary</key>
			<dimensions>
				<!-- A dictionary dimension with default dictionary -->
				<dimension>
					<key>Analytic</key>
					<class>MatchingWordsFromDictionary</class>
				</dimension>
				<dimension>
					<key>Clout</key>
					<class>MatchingWordsFromDictionary</class>
				</dimension>
				<dimension>
					<key>Authentic</key>
					<class>MatchingWordsFromDictionary</class>
				</dimension>
				<dimension>
					<key>Tone</key>
					<class>MatchingWordsFromDictionary</class>
				</dimension>
			</dimensions>
		</dimension>
		<!-- LanguageMetricsDimension -->
		<dimension>
			<key>lng-metrics</key>
			<dimensions>
				<dimension>
					<key>wps</key>
				</dimension>
				<dimension>
					<key>Sixltr</key>
					<class>WordsLongerThanNDimension</class>
					<wordlength>6</wordlength>
				</dimension>
				<dimension>
					<key>15ltr</key>
					<class>WordsLongerThanNDimension</class>
					<wordlength>15</wordlength>
				</dimension>
				<dimension>
					<key>Dic</key>
					<class>MatchingWordsFromDictionary</class>
					<dictionary>es</dictionary>
				</dimension>
			</dimensions>
		</dimension>
